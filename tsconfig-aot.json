{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true
  },
  "angularCompilerOptions": {
   		"genDir": "./aot",
   		"entryModule": "./src/app/app.module#AppModule",
    	"skipMetadataEmit": true
 }
}